  "presets": ["es2015", "react", "stage-1"],
  "plugins": ["transform-proto-to-assign"],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "compact": true,
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements",
      ]
    }
  }
}